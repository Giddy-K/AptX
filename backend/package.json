    "@google-cloud/vertexai": "^1.1.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "firebase-admin": "^12.7.0",
    "helmet": "^7.2.0",
    "joi": "^17.11.0",
